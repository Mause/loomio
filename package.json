{
  "name": "loomio",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Elliana May <me@mause.me>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "ts-node ./src/orch.ts",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "prebuild": "apollo client:codegen --target typescript --config apollo.config.json",
    "build": "tsc",
    "postbuild": "ts-node ./src/rewrite.ts",
    "introspect": "ts-node ./src/exercise.ts",
    "download": "apollo client:download-schema --config apollo.config.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@pipedream/platform": "^0.10.0",
    "@types/lodash": "^4.14.182",
    "axios": "0.27.2",
    "ironpipe": "^0.0.8",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.1",
    "@types/moxios": "^0.4.15",
    "@types/node": "~16.11.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@urql/core": "^2.4.4",
    "apollo": "^2.29.0-alpha.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-plugin-jest": "^26.1.4",
    "husky": ">=6",
    "jest": "^28.0.0",
    "lint-staged": ">=10",
    "moxios": "^0.4.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.0",
    "ts-morph": "^14.0.0",
    "ts-node": "^10.7.0"
  },
  "lint-staged": {
    "*.{js,ts}": "./node_modules/.bin/eslint --fix",
    "*.{js,css,md,ts,json}": "prettier --write"
  }
}
